{
    "scheme.isDark": false,

    "definitions": {
        "black": "#ff000000",
        "white": "#ffffff",
        "transparent": "#00000000",
        "21": "#212121",
        "31": "#313131",
        "75": "#757575",
        "9e": "#9e9e9e",
        "dd": "#dddddd",
        "f5": "#f5f5f5",
        "42": "#424242",
        "aa": "#aaaaaa",
        "cc": "#cccccc"
    },

    "editor": {
        "bg": "@21",

        "line.current": "@31",
        "line.divider": "@transparent",
        "line.num": "@aa",
        "line.num.current": "@f5",
        "line.num.bg": "@21",
        "line.num.panel": "@black",
        "line.num.panel.text": "@white",
        
        "text.normal": "@f5",
        "text.selected": "@21",
        "text.matched.bg": "#FF8F00",
        "text.selected.bg": "#ef9a9a",
        
        "snippet.bg.editing": "@cc",
        "snippet.bg.related": "#aadddddd",
        "snippet.bg.inactive": "#66dddddd",
        
        "scrollbar.thumb": "@75",
        "scrollbar.thumb.pressed": "#ff5252",
        "scrollbar.track": "#bdbdbd",
        
        "code.block.line": "@42",
        "code.block.line.current": "@white",
        "code.block.line.side": "@42",

        "highlighted.delimiters.underline": "#3f51b5",
        "highlighted.delimiters.bg": "#1d000000",
        "highlighted.delimiters.fg": "#dd000000",
        
        "completion.window.bg": "@75",
        "completion.window.outline": "@9e",
        
        "non_printable_char": "@dd"
    },

    "languages": [
        {
            "types": ["@log"],
            "styles": {
                "text.error": "#f44336",
                "text.warning": "#ffeb3b",
                "text.info": "#4caf50",
                "text.debug": "@f5",
                "text.verbose": "#4fc3f7",

                "error.priority": {
                    "fg": "@black",
                    "bg": "#f44336",
                    "completion": false
                },
                "warning.priority": {
                    "fg": "@black",
                    "bg": "#ffeb3b"
                },
                "info.priority": {
                    "fg": "@white",
                    "bg": "#1f65c0"
                },
                "debug.priority": {
                    "fg": "@white",
                    "bg": "#9e9d24"
                },
                "verbose.priority": {
                    "fg": "@black",
                    "bg": "@white"
                }
            }
        }
    ]
}